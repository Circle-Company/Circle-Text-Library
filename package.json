{
    "name": "circle-text-library",
    "version": "1.1.2",
    "description": "Biblioteca JavaScript/TypeScript para validação, extração e análise de texto, desenvolvida para o Circle App",
    "type": "module",
    "main": "./dist/src/index.js",
    "module": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/src/index.js",
            "require": "./dist/src/index.js",
            "types": "./dist/src/index.d.ts"
        },
        "./conversor": {
            "import": "./dist/src/classes/conversor/index.js",
            "require": "./dist/src/classes/conversor/index.js",
            "types": "./dist/src/classes/conversor/index.d.ts"
        },
        "./date": {
            "import": "./dist/src/classes/date/index.js",
            "require": "./dist/src/classes/date/index.js",
            "types": "./dist/src/classes/date/index.d.ts"
        },
        "./extractor": {
            "import": "./dist/src/classes/extractor/index.js",
            "require": "./dist/src/classes/extractor/index.js",
            "types": "./dist/src/classes/extractor/index.d.ts"
        },
        "./rich-text": {
            "import": "./dist/src/classes/rich.text/index.js",
            "require": "./dist/src/classes/rich.text/index.js",
            "types": "./dist/src/classes/rich.text/index.d.ts"
        },
        "./sentiment": {
            "import": "./dist/src/classes/sentiment/index.js",
            "require": "./dist/src/classes/sentiment/index.js",
            "types": "./dist/src/classes/sentiment/index.d.ts"
        },
        "./timezone": {
            "import": "./dist/src/classes/timezone/index.js",
            "require": "./dist/src/classes/timezone/index.js",
            "types": "./dist/src/classes/timezone/index.d.ts"
        },
        "./validator": {
            "import": "./dist/src/classes/validator/index.js",
            "require": "./dist/src/classes/validator/index.js",
            "types": "./dist/src/classes/validator/index.d.ts"
        },
        "./types": {
            "import": "./dist/src/types.js",
            "require": "./dist/src/types.js",
            "types": "./dist/src/types.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc && npm run build:types",
        "build:types": "tsc --emitDeclarationOnly --outDir dist",
        "prepublishOnly": "npm run build",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "clean": "rm -rf dist",
        "dev": "tsc --watch"
    },
    "keywords": [
        "text-analysis",
        "sentiment-analysis",
        "validators",
        "timezone",
        "text-processing",
        "validation",
        "extraction",
        "circle-app",
        "typescript",
        "javascript"
    ],
    "author": "Circle Company LTDA",
    "license": "CIRCLE-LICENSE-1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/circle-company/circle-text-library.git"
    },
    "bugs": {
        "url": "https://github.com/circle-company/circle-text-library/issues"
    },
    "homepage": "https://github.com/circle-company/circle-text-library#readme",
    "engines": {
        "node": ">=16.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.0",
        "@babel/core": "^7.4.0",
        "@babel/plugin-syntax-dynamic-import": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.4.0",
        "@babel/plugin-transform-spread": "^7.2.2",
        "@babel/preset-env": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "@vitest/coverage-v8": "^3.2.4",
        "babel-eslint": "^10.0.1",
        "babel-plugin-add-module-exports": "^1.0.2",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.10.0",
        "eslint-plugin-prettier": "^2.7.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-native": "^5.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "prettier": "^3.6.2",
        "typescript": "^5.0.0",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@babel/runtime": "^7.3.1",
        "core-js": "^2.5.0",
        "punycode": "1.4.1"
    }
}
